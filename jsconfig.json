{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "comp/*": [
        "src/components/*"
      ],
      "views/*": [
        "src/views/*"
      ],
      "langs/*": [
        "src/locales/i18n_json_files/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
